{
  "name": "projectbird",
  "private": true,
  "version": "0.0.0",
  "type": "module",  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "dev:host": "concurrently \"npm run server\" \"vite --host\"",
